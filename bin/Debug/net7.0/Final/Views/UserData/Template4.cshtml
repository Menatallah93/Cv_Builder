    <script src="/js/template4/modernizr.js"></script>

    <script src="/js/template4/jquery.flexslider.js"></script>
    <script src="/js/template4/waypoints.js"></script>
    <script src="/js/template4/jquery.fittext.js"></script>
    <script src="/js/template4/magnific-popup.js"></script>
    <script src="/js/template4/init.js"></script>

</body>

</html>













