    <!--- Basic Page Needs
    ================================================== -->
    <meta charset="utf-8">
    <title>Ceevee - Free Responsive HTML5/CSS3 Template</title>
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Mobile Specific Metas
    ================================================== -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- CSS
    ================================================== -->
    <link rel="stylesheet" href="/css/template4/default.css">
    <link rel="stylesheet" href="/css/template4/layout.css">
    <link rel="stylesheet" href="/css/template4/media-queries.css">
    <link rel="stylesheet" href="/css/template4/magnific-popup.css">
    <link href="https://fontawesome.com/icons/linkedin?f=brands&s=solid" rel="stylesheet">
    <link href="https://fontawesome.com/icons/github?f=brands&s=solid" rel="stylesheet">

    <!-- Script
    ================================================== -->

    <!-- Favicons
     ================================================== -->
    <link rel="shortcut icon" href="favicon.png">

</head>

<body>

    <!-- Header
    ================================================== -->
    <section id="headertemp4">

        
        <div class="row banner">
            <div class="banner-text">
                <h1 class="responsive-headline">I'm @Model.PersonalInfo.Name</h1>
                <h3>
                    @Model.PersonalInfo.JobTitle
                </h3>
                <div>

                    <a href="@Model.Links.GitHub" target="_blank"><i style="font-size:34px;    margin: 15px;color:white" class="fa-brands fa-github"></i></a>

                    <a href="@Model.Links.Linkedin" target="_blank"><i style="font-size:34px; color:blue" class="fa-brands fa-linkedin"></i></a>

                </div>
                <hr />

            </div>
        </div>

        <p class="scrolldown">
            <a class="smoothscroll" href="#about"><i class="icon-down-circle"></i></a>
        </p>

    </section> <!-- Header End -->
    <!-- About Section
    ================================================== -->
    <section id="about" style="text-align:center">

        <div class="row" style="margin-right:auto;margin-left:auto;display:block">

            <div class="col-md-3 columns">

                <img class="profile-pic" src="/img/clients/@Model.PersonalInfo.Photo" alt="" />

            </div>

            <div class="col-md-9 columns main-col text-center">
                
                     <h2>About Me</h2>

                     <p style="color:white">
                         @Model.CoverLetter
                      </p>
                

                <div class="row">

                    <div class="columns contact-details">

                        <h2>Contact Details</h2>
                        <p class="address" style="color:white">
                            <span>@Model.PersonalInfo.Name</span><br>
                            <span>
                                @Model.PersonalInfo.Address
                            </span><br>
                            <span>@Model.PersonalInfo.Phone</span><br>
                            <span><a href="mailto:@Model.PersonalInfo.Email" target="_blank">@Model.PersonalInfo.Email</a></span>
                        </p>

                    </div>

                    <div class="columns download">
                        <p>
                            <a href="#" class="button"><i class="fa fa-download"></i>Download Resume</a>
                        </p>
                    </div>

                </div> <!-- end row -->

            </div> <!-- end .main-col -->

        </div>

    </section> <!-- About Section End-->
    <!-- Resume Section
    ================================================== -->
    <section id="resume">

        <!-- Skills
        ----------------------------------------------- -->
        <div class="row skill" id="skills" style="margin-right:auto;margin-left:auto;display:block">

            <div class="col-md-3 columns header-col">
                <h1 style="text-align:left"><span>Skills</span></h1>
            </div>

            <div class="col-md-9 columns main-col">

                

                <div class="row" style="margin:50px 0;">
                    <div class="col-md-6">
                          @if (@Model.Skill[0].Name != null && @Model.Skill[0].Level != null)
                        {
                            <div class="mb-2">
                                <span>@Model.Skill[0].Name</span>
                                <div class="progress my-1" style="height:25px">
                                    <div class="progress-bar" role="progressbar" aria-valuemin="0" aria-valuemax="100" style="height:30px; width: @Model.Skill[0].Level%;"></div><span class="progress-value" style="margin-left:10px">@Model.Skill[0].Level%</span>
                                </div>
                            </div>
                        }
                          @if (@Model.Skill[1].Name != null && @Model.Skill[1].Level != null)
                        {
                            <div class="mb-2">
                                <span>@Model.Skill[1].Name</span>
                                <div class="progress my-1" style="height:25px">
                                    <div class="progress-bar" role="progressbar" aria-valuemin="0" aria-valuemax="100" style="height:30px; width: @Model.Skill[1].Level%;"></div><span class="progress-value" style="margin-left:10px">@Model.Skill[1].Level%</span>
                                </div>
                            </div>
                        }
                          @if (@Model.Skill[2].Name != null && @Model.Skill[2].Level != null)
                        {
                            <div class="mb-2">
                                <span>@Model.Skill[2].Name</span>
                                <div class="progress my-1" style="height:25px">
                                    <div class="progress-bar" role="progressbar" aria-valuemin="0" aria-valuemax="100" style="height:30px; width: @Model.Skill[2].Level%;"></div><span class="progress-value" style="margin-left:10px">@Model.Skill[2].Level%</span>
                                </div>
                            </div>
                        }
                    </div>
                    <div class="col-md-6">
                          @if (@Model.Skill[3].Name != null && @Model.Skill[3].Level != null)
                        {
                            <div class="mb-2">
                                <span>@Model.Skill[3].Name</span>
                                <div class="progress my-1" style="height:25px">
                                    <div class="progress-bar" role="progressbar" aria-valuemin="0" aria-valuemax="100" style="height:30px; width: @Model.Skill[3].Level%;"></div><span class="progress-value" style="margin-left:10px">@Model.Skill[3].Level%</span>
                                </div>
                            </div>
                        }
                          @if (@Model.Skill[4].Name != null && @Model.Skill[4].Level != null)
                        {
                            <div class="mb-2">
                                <span>@Model.Skill[4].Name</span>
                                <div class="progress my-1" style="height:25px">
                                    <div class="progress-bar" role="progressbar" aria-valuemin="0" aria-valuemax="100" style="height:30px; width: @Model.Skill[4].Level%;"></div><span class="progress-value" style="margin-left:10px">@Model.Skill[4].Level%</span>
                                </div>
                            </div>
                        }
                          @if (@Model.Skill[5].Name != null && @Model.Skill[5].Level != null)
                        {
                            <div class="mb-2">
                                <span>@Model.Skill[5].Name</span>
                                <div class="progress my-1" style="height:25px">
                                    <div class="progress-bar" role="progressbar" aria-valuemin="0" aria-valuemax="100" style="height:30px; width: @Model.Skill[5].Level%;"></div><span class="progress-value" style="margin-left:10px">@Model.Skill[5].Level%</span>
                                </div>
                            </div>
                        }
                    </div>
                </div>
                <!-- end skill-bars -->

            </div> <!-- main-col end -->

        </div>
        <!-- End skills -->


        <!-- experience -->
        <div class="row experience" id="Workexperience" style="margin-right:auto;margin-left:auto;display:block;text-align:left;margin-bottom:70px">

            <div class="three columns header-col">
                <h1><span>experience</span></h1>
            </div>

            <div class="nine columns main-col">

                 @if (@Model.Experiences[0].CompanyName != null && @Model.Experiences[0].StartDate != null && @Model.Experiences[0].EndDate != null && @Model.Experiences[0].JobTitle != null && @Model.Experiences[0].Details != null)
                {
                    <div class="row item">

                        <div class="twelve columns">

                            <h3 style="text-align:center">@Model.Experiences[0].CompanyName</h3>
                            <p class="info" style="text-align:center">@Model.Experiences[0].JobTitle <span>&bull;</span> <em class="date">@Model.Experiences[0].StartDate.ToShortDateString() - @Model.Experiences[0].EndDate.ToShortDateString()</em></p>

                            <p>
                                @Model.Experiences[0].Details
                            </p>

                        </div>

                    </div> <!-- item end -->
                }
                 @if (@Model.Experiences[1].CompanyName != null && @Model.Experiences[1].StartDate != null && @Model.Experiences[1].EndDate != null && @Model.Experiences[1].JobTitle != null && @Model.Experiences[1].Details != null)
                    {

                <div class="row item">

                    <div class="twelve columns">

                        <h3 style="text-align:center">@Model.Experiences[1].CompanyName</h3>
                        <p class="info" style="text-align:center">@Model.Experiences[1].JobTitle <span>&bull;</span> <em class="date">@Model.Experiences[1].StartDate.ToShortDateString() - @Model.Experiences[1].EndDate.ToShortDateString()</em></p>

                        <p>
                            @Model.Experiences[1].Details
                        </p>

                    </div>

                </div> <!-- item end -->
                }

                 @if (@Model.Experiences[2].CompanyName != null && @Model.Experiences[2].StartDate != null && @Model.Experiences[2].EndDate != null && @Model.Experiences[2].JobTitle != null && @Model.Experiences[2].Details != null)
                {
                    <div class="row item">

                        <div class="twelve columns">

                            <h3 style="text-align:center">@Model.Experiences[2].CompanyName</h3>
                            <p class="info" style="text-align:center">@Model.Experiences[2].JobTitle <span>&bull;</span> <em class="date">@Model.Experiences[2].StartDate.ToShortDateString() - @Model.Experiences[2].EndDate.ToShortDateString()</em></p>

                            <p>
                                @Model.Experiences[2].Details
                            </p>

                        </div>

                    </div> <!-- item end -->
                }

            </div> <!-- main-col end -->

        </div><!--End Experience-->


    

        <!-- Education
        ----------------------------------------------- -->
        <div class="row education" id="education" style="margin-right:auto;margin-left:auto;display:block;text-align:left">

            <div class="three columns header-col">
                <h1><span>Projects</span></h1>
            </div>

            <div class="nine columns main-col">
                 @if (@Model.Projects[0].Title != null && @Model.Projects[0].Description != null && @Model.Projects[0].Link != null && @Model.Projects[0].EndDate != null)
                {
                    <div class="row item">

                        <div class="twelve columns">

                            <h3 style="text-align:center">@Model.Projects[0].Title</h3>
                            <p class="info" style="text-align:center">@Model.Projects[0].Title <span>&bull;</span> <em class="date">@Model.Projects[0].EndDate.ToShortDateString()</em></p>

                            <p>
                                @Model.Projects[0].Description
                            </p>

                        </div>

                    </div> <!-- item end -->
                }

                 @if (@Model.Projects[1].Title != null && @Model.Projects[1].Description != null && @Model.Projects[1].Link != null && @Model.Projects[1].EndDate != null)
                {
                    <div class="row item">

                        <div class="twelve columns">

                            <h3 style="text-align:center">@Model.Projects[1].Title</h3>
                            <p class="info" style="text-align:center">@Model.Projects[1].Title <span>&bull;</span> <em class="date">@Model.Projects[1].EndDate.ToShortDateString()</em></p>

                            <p>
                                @Model.Projects[1].Description
                            </p>

                        </div>

                    </div> <!-- item end -->
                }

                 @if (@Model.Projects[2].Title != null && @Model.Projects[2].Description != null && @Model.Projects[2].Link != null && @Model.Projects[2].EndDate != null)
                {
                    <div class="row item">

                        <div class="twelve columns">

                            <h3 style="text-align:center">@Model.Projects[2].Title</h3>
                            <p class="info" style="text-align:center">@Model.Projects[2].Title <span>&bull;</span> <em class="date">@Model.Projects[2].EndDate.ToShortDateString()</em></p>

                            <p>
                                @Model.Projects[2].Description
                            </p>

                        </div>

                    </div> <!-- item end -->
                }

            </div> <!-- main-col end -->

        </div> <!-- End Education -->


    </section> <!-- Resume Section End-->
    <!-- footer
    ================================================== -->
    <!-- Java Script
    ================================================== -->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="/js/template4/jquery-1.10.2.min.js"><\/script>')</script>
